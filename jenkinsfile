pipeline {
    agent any

    stages {


        stage('Build') {
            steps {
                echo 'Startiong build'
                sh '/usr/local/go/bin/go build main.go'
            }
        }

        stage('Test') {
            steps {
                echo 'Checking existance of go binary'
                sh 'test -f main'
            }
        }
    }
}